        "eslint": "^9.0.0",
        "eslint-plugin-vue": "^10.6.2",
        "vite": "^7.1.5",
        "vue-eslint-parser": "^10.2.0"
    },
    "keywords": [
        "Kitware",
        "trame",
        "girder"
    ],
    "license": "Apache-2.0"
